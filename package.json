{
  "name": "brioso",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "functions"
  ],
  "scripts": {
    "dev": "concurrently \"vite --open\" \"firebase emulators:start\" \"yarn workspace functions watch\"",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src functions",
    "type-check": "tsc --pretty --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "plugin": "echo \"plugin dummy\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": "bash -c tsc --skipLibCheck --noEmit",
    "!(functions/**/*)*.@(ts|tsx|js|jsx)": "eslint --max-warnings 0",
    "*.@(ts|tsx|js|jsx|json|jsonc|json5|md|mdx|yaml|yml)": "prettier --write"
  },
  "dependencies": {
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "classnames": "^2.3.2",
    "firebase": "^9.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^11.4.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.11.2",
    "reactfire": "^4.2.2",
    "vite-plugin-svgr": "^2.4.0",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jsdom": "^21",
    "@types/prettier": "^2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "husky": "=8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "=13.2.1",
    "prettier": "^2.8.7",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.34.0"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "4.1.0"
  },
  "packageManager": "yarn@3.8.6"
}
